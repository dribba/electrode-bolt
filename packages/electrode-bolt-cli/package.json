    "findup-sync": "^0.3.0",
    "jsonfile": "^2.2.3",
    "mkdirp": "^0.5.1",
    "resolve": "^1.1.6",
    "step": "0.0.6"
  },
  "devDependencies": {
    "babel": "^5.8.23"
  }
}
