{
  "name": "electrode-bolt-cli",
  "version": "1.0.6",
  "description": "A CLI for electrode-bolt",
  "repository": "walmartreact/electrode-bolt",
  "bin": {
    "bolt": "bin/bolt"
  },
  "scripts": {
    "dev": "babel src/bolt-cli.js --watch --out-file lib/bolt-cli.js",
